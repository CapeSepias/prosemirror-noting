{
  "include": ["src/js/**.*s"],
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "allowJs": true,
    "declaration": true,
    "outDir": "dist",
    "emitDeclarationOnly": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  }
}
